---
import SectionCardIndex from "../assets/SectionCardIndex.astro";
import CoverImage from "./CoverImage.astro";
import HeroAbout from "./HeroAbout.astro";
import SocialNetworks from "./SocialNetworks.astro";
import Presentation from "./Presentation.astro";
import Description from "./Description.astro";
---

<SectionCardIndex>
  <CoverImage slot={"content-into-card"} />
  <HeroAbout slot={"content-into-card"} />
  <SocialNetworks slot={"content-into-card"} />

  <Presentation slot={"content-out-card"} isDark={false} />
  <Description slot={"content-out-card"} isDark={false} />
</SectionCardIndex>
