---
import ContentWrapperCard from "@shared/ContentWrapperCard.astro";
import CoverImage from "./components/CoverImage.astro";
import HeroAbout from "./components/HeroAbout.astro";
import SocialNetworks from "./components/SocialNetworks.astro";
import Presentation from "./components/Presentation.astro";
import Description from "./components/Description.astro";
---

<ContentWrapperCard>
  <CoverImage slot={"content-into-card"} />
  <HeroAbout slot={"content-into-card"} />
  <SocialNetworks slot={"content-into-card"} />

  <Presentation slot={"content-out-card"} isDark={false} />
  <Description slot={"content-out-card"} isDark={false} />
</ContentWrapperCard>
